{
    "name": "kawakattsun/duplicate-word-extraction",
    "require": {
        "php": ">=7.2.13",
        "vlucas/phpdotenv": "^3.3",
        "phpoffice/phpspreadsheet": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/classes/App/",
            "Csv\\": "src/classes/Csv/",
            "Excel\\" : "src/classes/Excel/"
        }
    },
    "scripts": {
        "phpunit": "phpunit"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.jp"
        }
    }
}
